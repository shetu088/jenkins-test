	{
	    list = new ArrayList<Integer>();
	    list1 = new ArrayList<Integer>();
	}
	
	public void addele()
	{
		list.add(3);
		list.add(1);
		list.add(11);
		list.add(19);
		list.add(17);
		list.add(19);
		
	}
	
	public List result()
	{
		for(Integer i : list)
		{
			if(i%2==0)
			{
				
			}
			System.out.println(i);
			
		}
		return list;
		
		
	}
	public static void main(String[] args) {
		// TODO Auto-generated method stub

